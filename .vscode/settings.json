{
    "search.exclude": {
        "### https://raw.github.com/github/gitignore/a392c48ccf4136ef75acb05b7ead4e9c75235c19/Global/OSX.gitignore": true,
        ".DS_Store": true,
        ".AppleDouble": true,
        ".LSOverride": true,
        "# Icon must end with two \\r": true,
        "Icon": true,
        "# Thumbnails": true,
        "._*": true,
        "# Files that might appear in the root of a volume": true,
        ".DocumentRevisions-V100": true,
        ".fseventsd": true,
        ".Spotlight-V100": true,
        ".TemporaryItems": true,
        ".Trashes": true,
        ".VolumeIcon.icns": true,
        "# Directories potentially created on remote AFP share": true,
        ".AppleDB": true,
        ".AppleDesktop": true,
        "Network Trash Folder": true,
        "Temporary Items": true,
        ".apdisk": true,
        "### https://raw.github.com/github/gitignore/a392c48ccf4136ef75acb05b7ead4e9c75235c19/Node.gitignore": true,
        "# Logs": true,
        "logs": true,
        "*.log": true,
        "npm-debug.log*": true,
        "# Runtime data": true,
        "pids": true,
        "*.pid": true,
        "*.seed": true,
        "# Directory for instrumented libs generated by jscoverage/JSCover": true,
        "lib-cov": true,
        "# Coverage directory used by tools like istanbul": true,
        "coverage": true,
        "# Grunt intermediate storage (http://gruntjs.com/creating-plugins#storing-task-files)": true,
        ".grunt": true,
        "# node-waf configuration": true,
        ".lock-wscript": true,
        "# Compiled binary addons (http://nodejs.org/api/addons.html)": true,
        "build/Release": true,
        "# Dependency directories": true,
        "node_modules": true,
        "jspm_packages": true,
        "# Optional npm cache directory": true,
        ".npm": true,
        "# Optional REPL history": true,
        ".node_repl_history": true,
        "### https://raw.github.com/github/gitignore/a392c48ccf4136ef75acb05b7ead4e9c75235c19/Global/Windows.gitignore": true,
        "# Windows image file caches": true,
        "Thumbs.db": true,
        "ehthumbs.db": true,
        "# Folder config file": true,
        "Desktop.ini": true,
        "# Recycle Bin used on file shares": true,
        "$RECYCLE.BIN/": true,
        "# Windows Installer files": true,
        "*.cab": true,
        "*.msi": true,
        "*.msm": true,
        "*.msp": true,
        "# Windows shortcuts": true,
        "*.lnk": true,
        "### https://raw.github.com/github/gitignore/a392c48ccf4136ef75acb05b7ead4e9c75235c19/Global/Linux.gitignore": true,
        "*~": true,
        "# temporary files which can be created if a process still has a handle open of a deleted file": true,
        ".fuse_hidden*": true,
        "# KDE directory preferences": true,
        ".directory": true,
        "# Linux trash folder which might appear on any partition or disk": true,
        ".Trash-*": true,
        "### Grimoire.js ###": true,
        "# Generated codes included in package publishing.": true,
        "lib": true,
        "lib-es5": true,
        "# Generated codes just used in local environment": true,
        "lib-ts": true,
        "lib-test": true,
        "lib-md": true,
        "ref": true,
        "register": true,
        "# If you are using atom-typescript without any configuration, that plugin would generate js files in ts folder.": true,
        "# To prevent conflict by this files, any js files included src folder would be ignored.": true,
        "# Only if you want to import js file from typescript(NOT RECOMMENDED, the ideal method is what separate them as packages and use them as dependency),you should remove the line below.": true,
        "src/**/*.js": true,
        "yarn.lock": true,
        "index.ts": true,
        "node_modules/gl-matrix/.rvmrc": true,
        "node_modules/gl-matrix/.DS_Store": true,
        "node_modules/gl-matrix/tmp": true,
        "node_modules/gl-matrix/node_modules": true,
        "node_modules/gl-matrix/npm-debug.log": true
    },
